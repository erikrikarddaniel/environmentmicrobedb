<%= form_for(@subject, :method => "put", :url=> project_subject_path(params[:project_id],@subject), :html => { :class => "form-inline"}) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="form">
    <%= f.text_field :code, :placeholder => "Code" %>
    <%= f.submit('Update Subject') %>
    </div>
<% end %>
