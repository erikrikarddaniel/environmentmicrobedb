<h1>Listing subject_properties</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @subject_properties.each do |subject_property| %>
  <tr>
    <td><%= subject_property.name %></td>
    <td><%= subject_property.value %></td>
    <td><%= link_to 'Show', subject_property %></td>
    <td><%= link_to 'Edit', edit_subject_property_path(subject_property) %></td>
    <td><%= link_to 'Destroy', subject_property, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

