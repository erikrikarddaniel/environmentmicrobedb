<li id="subprop_<%= subject_property.id %>">
	<% if (not params[:sample_id].nil?)%>
		<%= link_to 'Edit', edit_project_sample_set_sample_subject_property_path(params[:project_id],params[:sample_set_id],params[:sample_id],@subject,subject_property) %>
		<%= link_to 'Destroy', project_sample_set_sample_subject_property_path(params[:project_id],params[:sample_set_id],params[:sample_id],@subject,subject_property), confirm: 'Are you sure?', method: :delete %>
	<% else %>
		<%= link_to 'Edit', edit_project_subject_property_path(params[:project_id],@subject,subject_property) %>
		<%= link_to 'Destroy', project_subject_property_path(params[:project_id],@subject,subject_property), confirm: 'Are you sure?', method: :delete %>
	<% end %>
	<strong><%= subject_property.name %>:</strong>
	<%= subject_property.convert_value %>
</li>